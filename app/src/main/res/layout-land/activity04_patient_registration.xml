<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="73dp">


    <TextView
        android:id="@+id/editText04Id"
        android:layout_width="29dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:text="ID"
        app:layout_constraintBottom_toTopOf="@+id/text04Name"
        app:layout_constraintStart_toEndOf="@+id/guideline4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/edit04Id"
        android:layout_width="239dp"
        android:layout_height="17dp"
        android:layout_marginStart="8dp"
        android:text="aaa"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText04Id"
        app:layout_constraintStart_toEndOf="@+id/editText04Id"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text04Name"
        android:layout_width="29dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:text="氏名"
        app:layout_constraintBottom_toTopOf="@+id/text04Age"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText04Id"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText04Name"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:hint="氏名"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Name"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text04Age"
        android:layout_width="29dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:text="年齢"
        app:layout_constraintBottom_toTopOf="@+id/text04Affiliation"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text04Name"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text04Sex"
        android:layout_width="29dp"
        android:layout_height="27dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="160dp"
        android:text="性別"
        app:layout_constraintBottom_toTopOf="@+id/text04Affiliation"
        app:layout_constraintEnd_toStartOf="@+id/editText04Sex"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text04Name"
        app:layout_constraintVertical_bias="0.49"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText04Age"
        android:layout_width="91dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:hint="年齢"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Age"
        app:layout_constraintEnd_toStartOf="@+id/text04Sex"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText04Sex"
        android:layout_width="103dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:hint="性別"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Sex"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text04Affiliation"
        android:layout_width="29dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:text="所属"
        app:layout_constraintBottom_toTopOf="@+id/text04Detail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text04Age"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText04Affiliation"
        android:layout_width="239dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:hint="所属"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Affiliation"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text04Detail"
        android:layout_width="33dp"
        android:layout_height="27dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="16dp"
        android:text="詳細"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text04Affiliation" />


    <EditText
        android:id="@+id/editText04Detail"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:hint="詳細"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Detail"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="13dp" />

    <Button
        android:id="@+id/button04PatientRegistration"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/activity04_button_background_3"
        android:text="登録"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button04Clear"
        app:layout_constraintTop_toBottomOf="@+id/button04MovePatientList"
        app:layout_constraintVertical_bias="0.866" />

    <Button
        android:id="@+id/button04MovePatientList"
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/activity04_button_background_2"
        android:backgroundTint="@color/Main"
        android:text="患者一覧"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button04PatientRegistration"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toEndOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button04Clear"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/activity04_button_background"
        android:text="クリア"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button04PatientRegistration"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/guideline3"
        app:layout_constraintTop_toBottomOf="@+id/button04MovePatientList" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.30" />

    <TextView
        android:id="@+id/text04Kome01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Name"
        app:layout_constraintEnd_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/text04Kome02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Age"
        app:layout_constraintEnd_toStartOf="@+id/guideline2" />

    <TextView
        android:id="@+id/text04Kome03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/text04Sex"
        app:layout_constraintStart_toStartOf="@+id/guideline6" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="667dp"
        android:layout_height="wrap_content"
        android:entries="@array/PatientRegistrationlActivity_sex_list"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="198dp" />

</android.support.constraint.ConstraintLayout>